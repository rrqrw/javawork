package objecarray.dogs;

public class DogArray {

	public static void main(String[] args) {
		// Dog 인스턴스 4개를 생성해서 배열에 저장하고 
		// 정보를 출력하세요
         Dog[] dog = new Dog[4];
         // 배열에 저장(입력)
         dog[0]= new Dog("백구", "진돗개");
         dog[1]= new Dog("해피", "시츄");
         dog[2]= new Dog("대박", "불독");
         dog[3]= new Dog("사랑", "치와와");
       
         for(int i=0; i<dog.length; i++) {
        	 System.out.println(dog[i].getName()+","+
         dog[i].getType());
         }
	}
	
	 

}
